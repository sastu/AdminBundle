{% extends "AdminBundle:Base:layout.html.twig" %}

{% block section %}analitycs{% endblock %}


{% block content %}
    
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Analitycs
        <small>Inicia sesion con tu cuenta google analitycs</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="{{ path('optisoop_admin_default_index') }}"> <i class="ion-home"></i>  {{ "home" | trans }}</a></li>
        <li><a href="javascript:">{{ "catalogue" | trans }}</a></li>
        <li><a href="{{ path('optisoop_ecommerce_category_index') }}">{{ "category.plural" | trans }}</a></li>
      </ol>
    </section>
      
    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-body">
                <div class="alert alert-danger alert-dismissable">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <h4><i class="icon fa fa-ban"></i> Atención!</h4>
                        No dispone de una cuenta de google analitycss asociada. Por favor ingrese con otra cuenta de google.
                  </div>

            </div>
          </div>
        </div>
      </div>
    </section>
                        
                        
    

{% endblock %}